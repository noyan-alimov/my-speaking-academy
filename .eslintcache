 => {\n\t\te.preventDefault();\n\n\t\tif (!quizName) {\n\t\t\talert('Please provide a quiz name');\n\t\t\treturn;\n\t\t}\n\n\t\tif (user) {\n\t\t\tif (user.email) {\n\t\t\t\tconst id: string | void = await createQuiz(user.email, quizName);\n\t\t\t\tsetQuizName('');\n\t\t\t}\n\t\t} else {\n\t\t\thistory.push('/login');\n\t\t}\n\t};\n\n\treturn (\n\t\t<main className='flex flex-wrap'>\n\t\t\t<div className='w-100 flex flex-wrap'>\n\t\t\t\t<form onSubmit={createQuizSubmit} className='w-100 pv4'>\n\t\t\t\t\t<div className='w-100 ph4 ph7-ns'>\n\t\t\t\t\t\t<input\n\t\t\t\t\t\t\ttype='text'\n\t\t\t\t\t\t\tplaceholder='Name'\n\t\t\t\t\t\t\tname='quizName'\n\t\t\t\t\t\t\tvalue={quizName}\n\t\t\t\t\t\t\tonChange={e => {\n\t\t\t\t\t\t\t\tsetQuizName(e.target.value);\n\t\t\t\t\t\t\t}}\n\t\t\t\t\t\t\tclassName='w-100 pa2 f4 dark-blue'\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div className='w-100 ph5 mt4 ph7-ns flex justify-center'>\n\t\t\t\t\t\t<button\n\t\t\t\t\t\t\ttype='submit'\n\t\t\t\t\t\t\tclassName='w-100 w-50-ns bn pa3 f3 dim b bg-dark-blue near-white br2 pointer'\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\tCREATE QUIZ\n\t\t\t\t\t\t</button>\n\t\t\t\t\t</div>\n\t\t\t\t</form>\n\t\t\t\t<div className='w-100 flex flex-wrap'>\n\t\t\t\t\t{quizzes.map(quiz => (\n\t\t\t\t\t\t<Quiz\n\t\t\t\t\t\t\tkey={quiz.id}\n\t\t\t\t\t\t\tid={quiz.id}\n\t\t\t\t\t\t\tname={quiz.name}\n\t\t\t\t\t\t\tcreatorEmail={quiz.creatorEmail}\n\t\t\t\t\t\t/>\n\t\t\t\t\t))}\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</main>\n\t);\n};\n\nexport default Home;\n",["89","90"],"/Users/noyan/Desktop/personal-projects/msa-firebase/src/components/Quiz.tsx",[],"/Users/noyan/Desktop/personal-projects/msa-firebase/src/components/CreateQuestionForm.tsx",[],"/Users/noyan/Desktop/personal-projects/msa-firebase/src/components/Question.tsx",[],"/Users/noyan/Desktop/personal-projects/msa-firebase/src/firebase/db/questionDb.ts",[],"/Users/noyan/Desktop/personal-projects/msa-firebase/src/firebase/db/quizDb.ts",[],"/Users/noyan/Desktop/perso